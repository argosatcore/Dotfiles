URxvt's configuration:

!Locale:
	URxvt*imLocale: en_US.UTF-8

! Color palette:
	*.foreground:  #ebdbb2
	*.background:  #1d2021
	*.cursorColor:  #ebdbb2
	*.highlightColor:  #fb4934
	
	! black
	*.color0:      #1d2021
	*.color8:      #928374
	! red
	*.color1:      #cc241d
	*.color9:      #fb4934
	! green
	*.color2:      #98971a
	*.color10:     #b8bb26
	! yellow
	*.color3:      #d79921
	*.color11:     #fabd2f
	! blue
	*.color4:      #458588
	*.color12:     #83a598
	! magenta
	*.color5:      #b161286
	*.color13:     #d3869b
	! cyan
	*.color6:      #689d6a
	*.color14:     #8ec07c
	! white
	*.color7:      #a89984
	*.color15:     #ebdbb2

!Transparency
	URxvt*transparent:       false
	
	URxvt*depth: 32
	URxvt.background: [90]#14161C

!Cursor
URxvt.cursorBlink: true 

!Scrollbar
	URxvt*scrollBar:         false
	URxvt*scrollBar_right:   false
	
!Fonts:
	URxvt.font: xft:Fira Code Retina:pixelsize=17
!	URxvt.font: xft:SauceCodePro Nerd Font Mono:pixelsize=17
!	URxvt.font: xft:Monospace:pixelsize=18
!	URxvt.font: xft:Inconsolata:pixelsize=17

	
	URxvt.letterSpace: -4
	
	*antialias:  true
	*autohint:   true
	Xft.antialias:	1
	Xft.hinting:	1
	Xft.autohint:	0
	Xft.hintstyle:	hintslight
	Xft.rgba:	rgb
	Xft.lcdfilter:	lcddefault

!Perl Extensions:
	URxvt.perl-ext-common: default,matcher
	URxvt.url-launcher: /usr/bin/xdg-open
	
	URxvt.matcher.button: 1
	
	URxvt.perl-ext:      default,url-select
	URxvt.keysym.M-u:    perl:url-select:select_next
	URxvt.urlLauncher:   firefox
	URxvt.underlineURLs: true
	
! 	URxvt.perl-ext-common: tabbed
! 	URxvt.tabbed.tabbar-fg: 2
! 	URxvt.tabbed.tabbar-bg: 0
! 	URxvt.tabbed.tab-fg: 3
! 	URxvt.tabbed.tab-bg: 2

